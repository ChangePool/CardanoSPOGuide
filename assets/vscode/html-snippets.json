	"Import MDX Tabs Component": {
		"prefix": "Tabs Component",
		"body": [
			"import Tabs from '@theme/Tabs';",
			"import TabItem from '@theme/TabItem';"
		],
		"description": "Snippet for importing the MDX Tabs component in Docusaurus, required to display Tabs in Markdown"
	},

	"CoinCashew Guide Markdown Tabs": {
		"prefix": "Tabs",
		"body": [
			"<Tabs>",
			"\t<TabItem value=\"$1\" label=\"$2\" default>",
			"\t</TabItem>",
			"\t<TabItem value=\"$3\" label=\"$4\">",
			"\t</TabItem>",
			"</Tabs>"
		],
		"description": "Snippet for inserting tabs in Docusaurus Markdown. See also the Tabs Component snippet."
	},

	"CoinCashew Guide Markdown Image": {
		"prefix": "Image",
		"body": [
			"![$1](@site/static/img/$2)"
		],
		"description": "Snippet for inserting an image in Docusaurus Markdown"
	},

	"CoinCashew Guide OR Paragraph": {
