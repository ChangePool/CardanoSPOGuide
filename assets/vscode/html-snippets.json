	"CoinCashew Guide Import Raw Text File": {
		"prefix": "Raw Text File Import",
		"body": [
			"import CodeBlock from '@theme/CodeBlock';",
			"import $1 from '!!raw-loader!/txt/$2';"
		],
		"description": "Snippet for importing a raw text file, including a code block"
	},

		"CoinCashew Guide Code Block": {
		"prefix": "Code Block",
		"body": [
			"<CodeBlock language=\"bash\">",
			"\t{`$2`}",
			"</CodeBlock>"
		],
		"description": "Snippet for inserting a code block, including an imported text file. See also the Text File Import snippet."
	},

	"CoinCashew Guide Placeholder String Manipulation": {
		"prefix": "Placeholder String Manipulation",
		"body": [
			"var $1 = $2.replace(\"{{$3}}\", \"$4\");"
		],
		"description": "Snippet for replacing placeholders in strings with values"

	},

	"CoinCashew Guide Figure": {
		"prefix": "Figure",
		"body": [
			"<figure>",
			"\t<img src=\"img/$1\" />",
			"\t<figcaption>Figure $2</figcaption>",
			"</figure>"
		],
		"description": "Snippet for inserting a figure"
	},

	"CoinCashew Guide Table": {
		"prefix": "Table",
		"body": [
			"<table class=\"center\">",
			"\t<caption>Table 1 $1</caption>",
			"\t<tbody>",
			"\t\t<tr>",
			"\t\t\t<th width=\"50%\"><p>$2</p></th>",
			"\t\t\t<th width=\"50%\"><p>$3</p></th>",
			"\t\t</tr>",
			"\t\t<tr>",
			"\t\t\t<td class=\"center\"><p>$4</p></td>",
			"\t\t\t<td class=\"center\"><p>$5</p></td>",
			"\t\t</tr>",
			"\t</tbody>",
			"</table>"
		],
		"description": "Snippet for inserting a table"
	},

	"CoinCashew Guide Bookmark": {
		"prefix": "Bookmark",
		"body": [
			"<a id=\"$1\" />"
		],
		"description": "Snippet for inserting a bookmark"
	},

	"CoinCashew Guide Hyperlink": {
		"prefix": "Hyperlink",
		"body": [
			"<a href=\"$1\" target=\"_blank\">$2</a>"
