{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

	//
	// Dashes
	//
	"– en": {
    	"prefix": "--",
    	"body": "–"
	},
	
	"— em": {
    	"prefix": "---",
    	"body": "—"
	},

	//
	// HTML Entities
	//
	"& HTML Entity": {
		"prefix": "Ampersand",
		"body": "&amp;"
	},

	//"{ HTML Entity": {
	//	"prefix": "Left Brace",
	//	"body": "&#123;"
	//},

	//"} HTML Entity": {
	//	"prefix": "Right Brace",
	//	"body": "&#125;"
	//},

	"{ and } HTML Entities": {
		"prefix": "Curly Braces",
		"body": "&#123;$1&#125;"
	},

	"< HTML Entity": {
		"prefix": "Less Than",
		"body": "&lt;"
	},

	"> HTML Entity": {
		"prefix": "Greater Than",
		"body": "&gt;"
	},

	"< and > HTML Entities": {
		"prefix": "Carets",
		"body": "&lt;$1&gt;",
		"description": "Left and right carets"
	},

	"\" HTML Entity": {
		"prefix": "Quotation Mark",
		"body": "&quot;"
	},

	"' HTML Entity": {
		"prefix": "Apostrophe",
		"body": "&apos;"
	},

	"\" \" HTML Entity": {
		"prefix": "Non-breaking Space",
		"body": "&nbsp;"
	},

	//
	// Snippets for the CoinCashew Guide template using Docusaurus
	//

	"CoinCashew Guide Markdown New Topic": {
		"prefix": ["Markdown New Topic"],
		"body": [
			"---",
			"title: \"$1\"",
			"sidebar_label: \"$2\"",
			"slug: /$3",
			"hide_title: $4false",
			"hide_table_of_contents: $5false",
			"sidebar_position: $6",
			"description: \"$7\"",
			"---\n",
			"$8"
		],
		"description": "Template for new topics using Markdown in the CoinCashew Guide Web site created using Docusaurus"
	},

	"CoinCashew Guide HTML New Topic": {
		"prefix": ["HTML New Topic"],
		"body": [
			"---",
			"title: \"$1\"",
			"sidebar_label: \"$2\"",
			"slug: /$3",
			"hide_title: $4false",
			"hide_table_of_contents: $5true",
			"sidebar_position: $6",
			"description: \"$7\"",
			"---\n",
			"import React from 'react';\n",
			"export function HTMLContent() {",
			"\treturn (",
			"\t\t<div>\n",
			"\t\t\t$8\n",
		    "\t\t</div>",
			"\t);",
			"}",
			"",
			"<HTMLContent />"
		],
		"description": "Template for new topics using HTML in the CoinCashew Guide Web site created using Docusaurus"
	},

