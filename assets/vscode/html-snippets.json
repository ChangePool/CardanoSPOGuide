		],
		"description": "Snippet for inserting the word OR in Docusaurus HTML or Markdown"
	},

	"CoinCashew Procedure Introduction": {
		"prefix": "Procedure Introduction",
		"body": [
			"<p class=\"pi\">$1:</p>"
		],
		"description": "Snippet for inserting a procedure introduction"
	},

	"CoinCashew Guide Single-step Procedure": {
		"prefix": "Single-step Procedure",
		"body": [
			"<p class=\"pi\">To $1:</p>",
			"",
			"<ul class=\"ss\">",
			"\t<li>$2</li>",
			"</ul>"
		],
		"description": "Snippet for inserting a single-step procedure"
	},

	"Austral Sign": {
		"prefix": "ADA Symbol",
		"body": [
			"₳$1"
		],
		"description": "Snippet for inserting ₳"
	},

	"JSX Comment": {
		"prefix": "Comment",
